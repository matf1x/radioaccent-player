{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "id": "raccent",
  "name": "Radio Accent",
  "short_name": "Accent",
  "description": "Radio Accent is jouw radio met de beste hits van vroeger en nu.",
  "categories": ["radio", "audio", "entertainment"],
  "start_url": "./index.html",
  "theme_color": "#032d49",
  "background_color": "#262626",
  "display_override": ["window-control-overlay", "minimal-ui"],
  "display": "standalone",
  "orientation": "portrait",
  "icons": [
    {
      "src": "assets/app/icons/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "assets/app/icons/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "assets/app/icons/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "assets/app/icons/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "assets/app/screenshots/ra_screen_1.jpg",
      "type": "image/jpg",
      "sizes": "326x720",
      "form_factor": "narrow"
    },
    {
      "src": "assets/app/screenshots/ra_screen_1.jpg",
      "type": "image/jpg",
      "sizes": "330x720",
      "form_factor": "narrow"
    }
  ]
}